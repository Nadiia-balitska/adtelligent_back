generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feed {
  id         Int      @id @default(autoincrement())
  url        String   @unique
  title      String?
  items      Json     
  fetchedAt  DateTime @default(now())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model User {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  password   String
  name       String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
